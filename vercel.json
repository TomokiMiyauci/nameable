{
  "version": 2,
  "builds": [
    {
      "src": "website/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "/api/check_name.ts",
      "use": "vercel-deno@0.7.12"
    }
  ],
  "routes": [
    {
      "src": "/api/v1",
      "dest": "api/check_name.ts"
    },
    {
      "src": "/",
      "dest": "website"
    }
  ]
}
